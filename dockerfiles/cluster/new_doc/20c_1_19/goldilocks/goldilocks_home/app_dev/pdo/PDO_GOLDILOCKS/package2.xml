 This PECL release is the first release of GOLDILOCKS PDO Driver.
 </notes>
 <contents>
  <dir name="/">
   <file name="CREDITS" role="doc" />
   <file name="config.m4" role="src" />
   <file name="config.w32" role="src" />
   <file name="goldilocks_driver.c" role="src" />
   <file name="goldilocks_driver7.c" role="src" />
   <file name="goldilocks_stmt.c" role="src" />
   <file name="goldilocks_stmt7.c" role="src" />
   <file name="pdo_goldilocks.c" role="src" />
   <file name="php_pdo_goldilocks.h" role="src" />
   <file name="php_pdo_goldilocks_int.h" role="src" />
  </dir> <!-- / -->
 </contents>
 <dependencies>
  <required>
   <php>
    <min>5.1.0</min>
   </php>
   <pearinstaller>
    <min>1.4.0</min>
   </pearinstaller>
  </required>
 </dependencies>
 <providesextension>PDO_GOLDILOCKS</providesextension>
 <extsrcrelease>
  <configureoption name="with-pdo-goldilocks" default="$GOLDILOCKS_HOME" prompt="GOLDILOCKS installation directory?" />
 </extsrcrelease>
</package>
